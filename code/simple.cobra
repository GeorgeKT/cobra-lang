import cobra::syscalls

struct Point:
    var x = 0, y = 0

    pub func inc(self):
        self.x += 1
        self.y += 1

    func sum(self) -> int:
        return self.x + self.y

    pub func prod(self) -> int:
        return self.x * self.y + self.sum()


cobra::syscalls::write(STDOUT, "Running tests\n", 14)
var p = Point{4, 5}
p.inc()

exit(p.prod())
