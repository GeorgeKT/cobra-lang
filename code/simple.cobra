import cobra::syscalls

struct Point:
    var x = 0, y = 0

    pub func inc(self):
        self.x += 1
        self.y += 1

    pub func sum(self) -> int:
        return self.x + self.y

    pub func prod(self) -> int:
        return self.x * self.y

const NUMS = [1, 2, 3]
var X = Point{4, 4}

func main() -> int:
    write(STDOUT, "Running tests\n", 14)
    #var points = [Point{4, 5}, Point{1, 2}]
    var y = Point{5, 5}
    y.inc()
    return X.sum() + y.prod()
