extern print(s: string) -> int
extern println(s: string) -> int
extern close(fd: int) -> int

type File {
    fd: int,
}

~File(self) {
    println("I'm a destructor");
    close(self.fd);
}

File.read(self, bytes: int) -> string {
    "data"
}

type Box {
    inner: *$a,
}

box(inner: *$a) -> Box<$a> {
    Box{inner}
}

~Box(self) {
    delete self.inner;
}

