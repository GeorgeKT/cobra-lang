
interface Product {
    product(self) -> int
}

interface Sum {
    sum(self) -> int
}

interface Default {
    default() -> Self
}

type Point {
    x: int,
    y: int,
}

Point.sum(self) -> int {
    self.x + self.y
}

Point.product(self) -> int {
    self.x * self.y
}


