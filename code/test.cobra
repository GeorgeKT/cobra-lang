type Animal = Dog | Cat | Bird | Fish
type Option $a = Some{$a} | None

number(a: Animal) -> int =
    match a
        Dog => 7,
        Cat => 8,
        Bird => 22,
        Fish => 9,
        Some{i} => 10

unwrap_or(opt: Option $a, def: $a) -> $a =
    match opt
        Some{i} => i,
        None => def

main() -> int =
    number(Dog) + number(Fish) + number(Cat) + number(Bird) + unwrap_or(Some{7}, 9)
